<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
    crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.2/font/bootstrap-icons.min.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.18.0/font/bootstrap-icons.css" rel="stylesheet">
    <script src="https://code.iconify.design/iconify-icon/1.0.7/iconify-icon.min.js"></script>
  <link rel="stylesheet" href="/assets/styles/landingpage.css">
  <title>Origen Tattoo</title>
</head>

<body>
  <div id="backgroundimage">
    <div class="container text-center">
      <img src="/assets/images/miproyecto.png" alt="">
    </div>
    <div>
      <ul class="nav justify-content-center" style="align-items: center">
        <li class="nav-item">
          <a class="nav-link" href="#"></a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" href="#" style="opacity: auto;"><strong>INICIO</strong></a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" href="tattoocatalogue"><strong>TATUAJES</strong></a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" href="piercingscatalogue"><strong>PIERCINGS</strong></a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" href="#tattooartistspreview"><strong>ARTISTAS</strong></a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" href="#locationpreview"><strong>UBICACIÓN</strong></a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" href="#contactpreview"><strong>CONTACTO</strong></a>
        </li>

<% if (!locals.usuariologueado) { %>
        <nav class="navbar navbar-expand-sm justify-content-center">
          <ul class="navbar-nav mr-auto">
          </ul>
          <div class="div1" class="btn-toolbar">
            <a href="formregister" class="btn btn-dark fw-bold me-4 " role="button">REGISTRARSE</a>
            <a href="formlogin" class="btn btn-dark fw-bold " role="button">INICIAR SESIÓN</a>
          </div>
        </nav>
        <% } else { %>
        <nav class="navbar navbar-expand-sm justify-content-center">
          <ul class="navbar-nav mr-auto">
          </ul>
          <div class="div1" class="btn-toolbar">
            <a href="logout" class="btn btn-dark fw-bold me-4 " role="button">CERRAR SESIÓN</a>
            <% if (locals.rol == 'Cliente') { %>
              <a href="/clientprofile" class="btn btn-dark fw-bold me-4 " role="button"><i class="bi bi-person-circle"></i></a>
            <% } %>
            <% if (locals.rol == 'Administrador') { %>
              <a href="/admin" class="btn btn-dark fw-bold " role="button"><i class="bi bi-bar-chart-line"></i></a>
            <% } %>
          </div>
        </nav>
        <% } %>
      </ul>
    </div>
    <div class="container  text-center mt-5">
      <a href="makeabooking" class="btn btn-dark fw-bold" style="margin-top: 150px">AGENDAR CITA</a>
    </div>
  </div>

  <div class="d-flex flex-column p-md-2" id="tattooPreview">
    <h1 class="col-12">Tatuajes</h1>

    <div class="d-flex justify-content-center flex-wrap">
      <% tattoos.slice(0,4).forEach(tattoo=>{%>
        <div class="cardsizetattoos card col-md-6 col-lg-6 m-3 ms-4">
          <img src="<%=tattoo.imagen%>" class="card-img-top" alt="Tatuaje ojo con lineas">
          <div class="mt-2">
            <h5 class="font card-title">
              <%=tattoo.nombre%>
            </h5>
          </div>
          <div class="mt-2">
            <% tattooartists.forEach(tattooartist=>{%>
              <% if (tattoo.autor.toString()===tattooartist._id.toString()) { %>
                <a href="#" class="btn btn-dark" data-bs-toggle="modal" data-bs-target="#moreInfotattoo"
                  onclick="moreInfoTattoo(`<%= tattoo.nombre %>`, `<%= tattoo.descripcion%>`, `<%= tattoo.tamano%>`, `<%= tattoo.color %>`, `<%= tattoo.tecnica%>`, `<%=tattooartist.alias%>`, `<%= tattoo.imagen%>`)">Más
                  información</a>
                <% } %>
                  <%})%>
          </div>
        </div>
        <%})%>
    </div>

    <div class="mt-5">
      <a href="tattoocatalogue" class="btn btn-outline-dark fw-bold ">Ver más</a>
    </div>

  </div>

  <div class="p-md-4 " id="piercingspreview">
    <h1 class="col-12">Piercings</h1>

    <div class="d-flex justify-content-center flex-wrap">
      <% piercings.slice(0,4).forEach(piercing=>{%>
        <div class="cardsizepiercings card col-md-6 col-lg-6 m-2 ms-4">
          <img src="<%=piercing.imagen%>" class="card-img-top" alt="Snake bites">
          <div class="mt-2">
            <h5 class="font card-title">
              <%=piercing.nombre%>
            </h5>
          </div>
          <div class="mt-2">
            <a href="#" class="btn btn-dark" data-bs-toggle="modal" data-bs-target="#moreInfopiercing"
              onclick="moreInfoPiercing(`<%= piercing.nombre %>`, `<%= piercing.zona%>`, `<%= piercing.imagen%>`)">Más
              información</a>
          </div>
        </div>
        <%})%>
    </div>

    <div class="mt-5">
      <a href="piercingscatalogue" class="btn btn-outline-dark fw-bold ">Ver más</a>
    </div>
  </div>

  <div class="p-md-4" id="tattooartistspreview">
    <h1 class="col-12">Artistas</h1>

    <div class="d-flex justify-content-center flex-wrap">
      <% tattooartists.slice(0,8).forEach(tattooartist=>{%>
        <div class="cardsizetattooartists card col-md-6 col-lg-6 m-2 ms-4">
          <img src="<%=tattooartist.fotoTatuador%>" class="imageartists card-img-top" alt="Snake bites">
          <div class="mt-2">
            <h5 class="font card-title">
              <%=tattooartist.alias%>
            </h5>
          </div>
          <div class="mt-2">
            <a href="#" class="btn btn-dark" data-bs-toggle="modal" data-bs-target="#moreInfotattooartists"
              onclick="moreInfoTattooartists(`<%= tattooartist.alias %>`, `<%= tattooartist.celular%>`, `<%= tattooartist.correo%>`, `<%= tattooartist.fotoTatuador%>`)">Más
              información</a>
          </div>
        </div>
        <%})%>
    </div>
  </div>

  <div class="p-md-4" id="locationpreview">
    <h1 class="col-12">Ubicaciones</h1>

    <div class="d-flex justify-content-center flex-wrap">

      <div class="location card col-md-6 col-lg-6 m-3">
        <div class="embed-responsive embed-responsive-1by1 w-100 h-100 ">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3966.5020943174563!2d-75.5735708!3d6.1972914999999995!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8e46831905929e9b%3A0x2434302231041cab!2sORIGEN%20Tattoo%20-%20C.C.%20Santa%20Fe!5e0!3m2!1ses-419!2sco!4v1700926829225!5m2!1ses-419!2sco"
            class="embed-responsive-item w-100 h-100 " allowfullscreen="" loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
        <div>
          <h5 class="font card-title">C.C Santa Fe</h5>
        </div>
      </div>


      <div class="location card col-md-6 col-lg-6 m-3">
        <div class="embed-responsive embed-responsive-1by1 w-100 h-100 ">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3966.2613007930263!2d-75.57319972427139!3d6.2292413265113575!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8e44284a5d344ad7%3A0x9364bdfaa7f5c8f!2sORIGEN%20Tattoo%20-%20C.C.%20Premium%20Plaza!5e0!3m2!1ses-419!2sco!4v1700927861459!5m2!1ses-419!2sco"
            class="embed-responsive-item w-100 h-100 " allowfullscreen="" loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
        <div>
          <h5 class="font card-title">C.C Premium plaza</h5>
        </div>
      </div>

    </div>
  </div>

  <div class="modal fade" id="moreInfotattoo" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
    aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg">
      <div class="modal-content">
        <div class="d-flex modal-body">
          <div class="w-50">
            <img alt="blackoutflores" class="w-100 " id="imagentattoo">
          </div>
          <div class="d-flex flex-column w-50  ">
            <h2 class="modalfont ms-3" id="nombretattoo"></h2>
            <div class="ms-3 mt-2 ">
              <h6>Descripción:</h6>
              <p class="text-justify" id="descripciontattoo"></p>
              <h6>Tamaño:</h6>
              <p id="tamanotattoo"></p>
              <h6>Color:</h6>
              <p id="colortattoo"></p>
              <h6>Tecnica:</h6>
              <p id="tecnicatattoo"></p>
              <h6>Autor:</h6>
              <p id="autortattoo"></p>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-outline-danger" data-bs-dismiss="modal">Cerrar</button>
        </div>
      </div>
    </div>
  </div>

  <div class="modal fade" id="moreInfopiercing" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
    aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg">
      <div class="modal-content">
        <div class="d-flex modal-body">
          <div class="w-50">
            <img class="w-100 " id="imagenpiercing">
          </div>
          <div class="d-flex flex-column w-50  ">
            <h2 class="modalfont ms-3" id="nombrepiercing"></h2>
            <div class="ms-3 mt-2 ">
              <h6>Zona:</h5>
                <p id="zonapiercing"></p>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-outline-danger" data-bs-dismiss="modal">Cerrar</button>
        </div>
      </div>
    </div>
  </div>

  <div class="modal fade" id="moreInfotattooartists" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
    aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg">
      <div class="modal-content">
        <div class="d-flex modal-body">
          <div class="w-50">
            <img class="w-100 " id="fototatuador">
          </div>
          <div class="d-flex flex-column w-50  ">
            <h2 class="modalfont ms-3" id="aliastatuador"></h2>
            <div class="ms-3 mt-2 ">
              <h6>Celular:</h5>
                <p id="celulartatuador"></p>
                <h6>Correo:</h6>
                <p id="correotatuador"></p>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-outline-danger" data-bs-dismiss="modal">Cerrar</button>
        </div>
      </div>
    </div>
  </div>

  <div class="contenedor">
        <a href="https://www.instagram.com/origentattoostudio/" target="_blank" class="btn-btn-outline sticky-button"><iconify-icon icon="skill-icons:instagram"></iconify-icon></button>
  </div>

</body>

<footer class="bg-body-tertiary text-center">
  <div class="p-4">
    <section class="mb-4">
      <!-- Facebook -->
      <a data-mdb-ripple-init class="logos btn btn-outline btn-floating m-1 btn-lg" href="https://www.instagram.com/origentattoostudio/" target="_blank" role="button"
        ><iconify-icon icon="ic:baseline-facebook"></iconify-icon></a>

      <!-- Whatsapp -->
      <a data-mdb-ripple-init class="logos btn btn-outline btn-floating m-1 btn-lg" href="https://api.whatsapp.com/send/?phone=573160552930&text&type=phone_number&app_absent=0" target="_blank" role="button"
      ><iconify-icon icon="ic:baseline-whatsapp"></iconify-icon></a>

      <!-- Instagram -->
      <a data-mdb-ripple-init class="logos btn btn-outline btn-floating m-1 btn-lg" href="https://es-la.facebook.com/origentattoostudio" role="button"
        ><iconify-icon icon="mdi:instagram"></iconify-icon></a>

    <div class="font">
      <h2 id="contactpreview">Contacto</h2>
      </div>
  </div>

  <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.05);">
    © 2023 Copyright
  </div>
  <!-- Copyright -->
</footer>
<!-- Footer -->
<script src="/assets/js/moreinfotattoo.js"> </script>
<script src="/assets/js/moreinfopiercing.js"></script>
<script src="/assets/js/moreinfotattooartists.js"></script>


</html>